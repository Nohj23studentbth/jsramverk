<%- include('includes/header'); %>
<h2>Dokuments</h2>
<div class="container"> 
    <div class="doc">
        <h3><a href="/doc">Create new dockument</a></h3>
    </div>
    <% if (docs && docs.length > 0) { %>
        <% docs.forEach(function(doc) { %>
            <div class="doc">
                <h3><a href="/doc/<%= doc.id %>"><%= doc.title %></a></h3>
            </div>
        <% }); %>
    <% } %>
</div>   
<%- include('includes/footer'); %>
